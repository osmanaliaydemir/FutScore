{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.bundle.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/jquery.dataTables.min.css"
      ]
    },
    {
      "library": "datatables.net-buttons@2.4.2",
      "destination": "wwwroot/lib/datatables-buttons/",
      "files": [
        "js/buttons.html5.min.js",
        "js/buttons.print.min.js",
        "js/dataTables.buttons.min.js"
      ]
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "js/select2.min.js",
        "css/select2.min.css"
      ]
    },
    {
      "library": "sweetalert2@11.10.5",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "font-awesome@6.5.1",
      "destination": "wwwroot/lib/fontawesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-brands-400.woff2"
      ]
    },
    {
      "library": "jszip@3.10.1",
      "destination": "wwwroot/lib/jszip/",
      "files": [
        "jszip.min.js"
      ]
    },
    {
      "library": "pdfmake@0.2.9",
      "destination": "wwwroot/lib/pdfmake/",
      "files": [
        "pdfmake.min.js",
        "vfs_fonts.js"
      ]
    }
